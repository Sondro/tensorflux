<xml>
   <set name="static_link" value="1" />
   <include name="${HXCPP}/toolchain/haxe-target.xml" />

   <section if="GPU" >
     <setup name="nvcc" />
     <nvcclinkflag value="-arch=compute_30" />
     <section if="windows_host" >
       <!--
       tf assumes in various places header files to be in cuda/include. On windows the cuda sdk
        installs them under cuda/version/include and to avoid that we need to change tf we copy a
        few files to cuda/include
       -->
       <mkdir name="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/cuda.h" to="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/cuComplex.h" to="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/curand.h" to="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/cublas_v2.h" to="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/cufft.h" to="gen/cuda/include" />
       <copy from="${CUDA_SDK}/include/cuda_runtime_api.h" to="gen/cuda/include" />
       <!--<copy from="${CUDNN_HOME}/include/cudnn.h" to="gen/cuda/include" />-->
        <nvccflag value="-Igen" />

     </section>
   </section>

   <files id="tensorflow" dir="../modules/tensorflow" >
